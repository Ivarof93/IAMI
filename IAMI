        for cell in row:
            cell.border = border_style

    # Llenar la tabla PRIMERA VEZ
    for row, regime in enumerate(['CONTRIBUTIVO', 'SUBSIDIADO'], start=5):
        consult_sheet.cell(row=row, column=1, value=regime)
        col = 2
        for age in ages:
